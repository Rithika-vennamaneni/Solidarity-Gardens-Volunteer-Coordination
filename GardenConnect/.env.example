# Neon PostgreSQL Database URL
# Get this from your Neon dashboard: https://console.neon.tech/
DATABASE_URL=postgresql://username:password@your-neon-hostname.neon.tech/dbname?sslmode=require

# Stack Auth Configuration
# Get these from your Stack Auth dashboard: https://app.stack-auth.com/
STACK_PROJECT_ID=your_stack_project_id
STACK_SECRET_KEY=your_stack_secret_key

# Server Configuration
PORT=5000
NODE_ENV=development

# Development Options
# Set to 'true' to bypass authentication in development mode
BYPASS_AUTH=false
